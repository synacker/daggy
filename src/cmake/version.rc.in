1               VERSIONINFO
FILEVERSION     @VERSION@
PRODUCTVERSION  @VERSION@
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "FileVersion",      "@VERSION_EXTENDED@\0"
            VALUE "ProductVersion",   "@VERSION_EXTENDED@\0"
            VALUE "ProductName",      "@PROJECT_NAME@\0"
            VALUE "LegalCopyright",   "@PROJECT_COPYRIGTH@"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END